{
  "name": "gstat-map",
  "version": "2.0.2",
  "description": "G-Stat is a vue-based library built around Vue2Leaflet, which can display reactive areas and markers on a Leaflet map",
  "repository": {
    "type": "git",
    "url": "https://github.com/MinerMax555/G-Stat"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "serve": "vue-cli-service serve testing/main.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "lint": "vue-cli-service lint --no-fix",
    "version": "npm run build"
  },
  "prepublishOnly": "npm run build",
  "dependencies": {
    "leaflet.markercluster": "~1.5.3",
    "@gstat/leaflet-gesture-handling": "^1.2.8"
  },
  "devDependencies": {
    "@betit/rollup-plugin-rename-extensions": "^0.1.0",
    "@rbnlffl/rollup-plugin-eslint": "^4.0.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.0.1",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/leaflet.markercluster": "^1.5.1",
    "@types/leaflet": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-typescript": "^11.0.1",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-vue": "^9.4.0",
    "leaflet": "1.8.0",
    "lint-staged": "^13.0.3",
    "minimist": "^1.2.6",
    "rollup": "^2.79.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-typescript2": "^0.34.0",
    "rollup-plugin-vue": "^5.1.9",
    "typescript": "^4.8.3",
    "vue": "~2.6.14",
    "vue2-leaflet": "^2.7.1",
    "vue-template-compiler": "~2.6.14"
  },
  "peerDependencies": {
    "@types/geojson": "^7946.0.10",
    "@types/leaflet": "^1.7.11",
    "leaflet": "1.8.0",
    "vue": "^2.6.14",
    "vue2-leaflet": "^2.7.1"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "sideEffects": [
    "*.css"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
