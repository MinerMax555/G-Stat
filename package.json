{
  "name": "gstat-map",
  "version": "1.7.6",
  "description": "G-Stat is a vue-based library built around Vue2Leaflet, which can display reactive areas and markers on a Leaflet map",
  "repository": {
    "type": "git",
    "url": "https://github.com/MinerMax555/G-Stat"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "serve": "vue-cli-service serve testing/main.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "lint": "vue-cli-service lint --no-fix",
    "version": "npm run build"
  },
  "prepublishOnly": "npm run build",
  "dependencies": {
    "@glartek/leaflet.markercluster": "^1.4.4",
    "@gstat/leaflet-gesture-handling": "^1.2.3"
  },
  "devDependencies": {
    "@betit/rollup-plugin-rename-extensions": "^0.1.0",
    "@rbnlffl/rollup-plugin-eslint": "^1.1.4",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/leaflet": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-typescript": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/eslint-config-typescript": "^7.0.0",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "leaflet": "^1.7.1",
    "lint-staged": "^10.5.4",
    "minimist": "^1.2.5",
    "rollup": "^2.42.3",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^5.1.9",
    "typescript": "^4.2.3",
    "vue": "^2.6.12",
    "vue2-leaflet": "^2.6.0",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "@types/geojson": "^7946.0.7",
    "@types/leaflet": "^1.7.0",
    "leaflet": "^1.7.1",
    "vue": "^2.6.12",
    "vue2-leaflet": "^2.6.0"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "sideEffects": [
    "*.css"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
